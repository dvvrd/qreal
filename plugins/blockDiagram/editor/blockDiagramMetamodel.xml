  <metamodel xmlns="http://schema.real.com/schema/">
	<include>../../commonMetamodels/kernelMetamodel.xml</include>
	<include>../../commonMetamodels/basicBehaviorsMetamodel.xml</include>
	<include>../../commonMetamodels/basicActionsMetamodel.xml</include>
	<namespace>UML 2.0</namespace>
	<diagram displayedName="Block Diagram" nodeName="BlockDiagramNode" name="BlockDiagram">
	  <graphicTypes>
		<node displayedName="Block Diagram" name="BlockDiagramNode">
		  <graphics>
			<external file="qml/blockDiagramNode.qml"/>
			<labels>
			  <label x="40" y="10" textBinded="name"/>
			</labels>
			<ports>
			  <pointPort x="0" y="100"/>
			  <pointPort x="100" y="0"/>
			  <pointPort x="200" y="100"/>
			  <pointPort x="100" y="200"/>
			</ports>
		  </graphics>
		  <logic>
			<generalizations>
			  <parent parentName="KernelDiagram::Diagram"/>
			</generalizations>
			<container>
			  <contains type="BlockDiagram::ActionElement"/>
			  <contains type="BlockDiagram::BlockFinalNode"/>
			  <contains type="BlockDiagram::ControlFlow"/>
			  <contains type="BlockDiagram::InitialNode"/>
			  <contains type="BlockDiagram::ConditionNode"/>
			</container>
		  </logic>
		</node>
		<node displayedName="Action" name="ActionElement">
		  <graphics>
			<external file="qml/actionElement.qml"/>
			<ports>
			  <linePort>
				<start startx="79" starty="10"/>
				<end endx="79" endy="50"/>
			  </linePort>
			  <linePort>
				<start startx="1" starty="10"/>
				<end endx="1" endy="50"/>
			  </linePort>
			  <linePort>
				<start startx="10" starty="1"/>
				<end endx="70" endy="1"/>
			  </linePort>
			  <linePort>
				<start startx="10" starty="59"/>
				<end endx="70" endy="59"/>
			  </linePort>
			</ports>
			<labels>
			  <label textBinded="process" center="true"/>
			</labels>
		  </graphics>
		  <logic>
			<properties>
			  <property type="string" name="process" />
			</properties>
		  </logic>
		</node>
		<node displayedName="Final Node" name="BlockFinalNode">
		  <graphics>
			<external file="qml/blockFinalNode.qml"/>
			<ports>
			  <pointPort x="0" y="15"/>
			  <pointPort x="15" y="0"/>
			  <pointPort x="15" y="30"/>
			  <pointPort x="30" y="15"/>
			  <pointPort x="15" y="15"/>
			</ports>
		  </graphics>
		  <logic>
		  </logic>
		</node>
		<edge displayedName="Control Flow" name="ControlFlow">
		  <graphics>
			<lineType type="solidLine"/>
		  </graphics>
		  <logic>
			<possibleEdges>
			  <possibleEdge beginName="InitialNode" directed="true" endName="ConditionNode"/>
			  <possibleEdge beginName="InitialNode" directed="true" endName="BlockFinalNode"/>
			  <possibleEdge beginName="InitialNode" directed="true" endName="ActionElement"/>
			  <possibleEdge beginName="ActionElement" directed="true" endName="BlockDiagramNode"/>
			  <possibleEdge beginName="ActionElement" directed="true" endName="ActionElement"/>
			  <possibleEdge beginName="ActionElement" directed="true" endName="ConditionNode"/>
			  <possibleEdge beginName="ActionElement" directed="true" endName="BlockFinalNode"/>
			</possibleEdges>
			<associations endType="open_arrow" beginType="no_arrow" />
		  </logic>
		</edge>
		<edge displayedName="Condition Control Flow" name="ConditionControlFlow">
		  <graphics>
			<labels>
			  <label textBinded="type"/>
			</labels>
			<lineType type="solidLine"/>
		  </graphics>
		  <logic>
			<properties>
			  <property type="bool" name="type">
				<default>false</default>
			  </property>
			</properties>
			<possibleEdges>
			  <possibleEdge beginName="ConditionNode" directed="true" endName="ConditionNode"/>
			  <possibleEdge beginName="ConditionNode" directed="true" endName="ActionElement"/>
			  <possibleEdge beginName="ConditionNode" directed="true" endName="BlockFinalNode"/>
			</possibleEdges>
			<associations endType="open_arrow" beginType="no_arrow" />
		  </logic>
		</edge>
		<node displayedName="Initial Node" name="InitialNode">
		  <graphics>
			<external file="qml/initialNode.qml"/>
			<ports>
			  <pointPort x="0" y="15"/>
			  <pointPort x="30" y="15"/>
			  <pointPort x="15" y="0"/>
			  <pointPort x="15" y="30"/>
			  <pointPort x="15" y="15"/>
			</ports>
		  </graphics>
		  <logic>
		  </logic>
		</node>
		<node displayedName="Condition Node" name="ConditionNode">
		  <graphics>
			<external file="qml/conditionNode.qml"/>
			<ports>
			  <pointPort x="1" y="30"/>
			  <pointPort x="30" y="59"/>
			  <pointPort x="59" y="30"/>
			  <pointPort x="30" y="1"/>
			</ports>
			<labels>
			  <label textBinded="condition" center="true"/>
			</labels>
		  </graphics>
		  <logic>
			<properties>
			  <property type="string" name="condition" />
			</properties>
		  </logic>
		</node>
	  </graphicTypes>
	  <palette>
		  <group name="Элементы">
			  <element name="ConditionNode"/>
			  <element name="BlockDiagramNode"/>
			  <element name="ActionElement"/>
			  <element name="InitialNode"/>
			  <element name="ConditionControlFlow"/>
			  <element name="ControlFlow"/>
			  <element name="BlockFinalNode"/>
		  </group>
	  </palette>
	</diagram>
  </metamodel>
