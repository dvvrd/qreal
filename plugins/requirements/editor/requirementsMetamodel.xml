<metamodel xmlns="http://schema.real.com/schema/" >
  <include>../../commonMetamodels/kernelMetamodel.xml</include>
  <namespace>UML 2.0</namespace>
  <diagram displayedName="Requirements Diagram" nodeName="RequirementsDiagramNode" name="RequirementsDiagram">
    <nonGraphicTypes>
      <enum displayedName="Type" name="reqnType">
        <value>Feature</value>
        <value>UseCase</value>
        <value>Supplementary</value>
        <value>Behaviour</value>
      </enum>
      <enum displayedName="Priority" name="reqnPriority">
        <value>High</value>
        <value>Medium</value>
        <value>Low</value>
        <value>NA</value>
      </enum>
    </nonGraphicTypes>
    <graphicTypes>
      <import as="Comment" displayedName="Comment" name="KernelDiagram::Comment"/>
      <import as="CommentLink" displayedName="Comment Link" name="KernelDiagram::CommentLink"/>
      <node displayedName="Requirements Diagram" path="10, 0 : 90, 0 : 90, 100 : 10, 100 : 0, 90 : 0, 10 : 100, 10 : 100, 90 : " name="RequirementsDiagramNode">
        <graphics>
          <picture sizex="200" sizey="200">
				<external file="qml/RequirementsDiagramNodeClass.qml"/>
		  </picture>
          <labels>
            <label x="40" y="10" textBinded="name"/>
          </labels>
          <ports>
            <pointPort x="0" y="100"/>
            <pointPort x="100" y="0"/>
            <pointPort x="200" y="100"/>
            <pointPort x="100" y="200"/>
          </ports>
        </graphics>
        <logic>
          <generalizations>
            <parent parentName="KernelDiagram::Diagram"/>
          </generalizations>
          <container>
            <contains type="RequirementsDiagram::reqnFeatured"/>
            <contains type="RequirementsDiagram::reqnConceptAlternative"/>
            <contains type="RequirementsDiagram::reqnLeaf"/>
            <contains type="RequirementsDiagram::reqnParent"/>
            <contains type="RequirementsDiagram::reqnParentMandatory"/>
            <contains type="RequirementsDiagram::reqnParentOr"/>
            <contains type="RequirementsDiagram::reqnParentOptional"/>
            <contains type="RequirementsDiagram::reqnParentAlternative"/>
            <contains type="RequirementsDiagram::reqeP2N"/>
          </container>
        </logic>
      </node>
      <node name="reqnFeatured" displayedName="Featured Element" >
        <graphics>

          <!--<description>as leaf - for the possibility of future node extension</description>-->
          <picture sizex="30" sizey="30" >
            <external file="qml/reqnFeaturedClass.qml"/>
          </picture>
          <labels>
            <label x="5" y="10" textBinded="name"/>
          </labels>
          <ports>
            <pointPort x="5" y="15" />
            <pointPort x="15" y="5" />
            <pointPort x="25" y="15" />
            <pointPort x="15" y="25" />
            <pointPort x="15" y="15" />
          </ports>

        </graphics>
        <logic>
          <properties>
            <property type="string" name="description" />
            <property type="reqnPriority" name="priority" />
            <!--<property>
              <name>source</name>
              <type>string</type>
              <description>requirement source value</description>
            </property>-->
            <property type="string" name="source" />
            <property type="reqnType" name="type" />
          </properties>
          <!---<associations>
          <assoc_ref idref="aP2N_Featured" />
        </associations>-->
          <generalizations>

            <parent parentName="KernelDiagram::KernelElement" />

          </generalizations>
          <connections>
            <connection type="RequirementsDiagram::RequirementsDiagramNode"/>
          </connections>
        </logic>
      </node>
      <node name="reqnConceptAlternative" displayedName="Concept Alternative" >
        <graphics>

          <!--<description>concept/leaf/feature graphical representation</description>-->
          <picture sizex="30" sizey="30" >
            <external file="qml/reqnConceptAlternativeClass.qml" />
          </picture>
          <labels>
            <label x="5" y="10" textBinded="name"/>
          </labels>
          <ports>
            <pointPort x="5" y="15" />
            <pointPort x="15" y="5" />
            <pointPort x="25" y="15" />
            <pointPort x="15" y="25" />
            <pointPort x="15" y="15" />
          </ports>

        </graphics>
        <logic>
          <generalizations>

            <parent parentName="reqnFeatured" />

          </generalizations>

        </logic>
      </node>
      <node name="reqnLeaf" displayedName="Leaf" >
        <graphics>

          <picture sizex="30" sizey="30" >
            <external file="qml/reqnLeafClass.qml"/>
          </picture>
          <labels>
            <label x="5" y="10" textBinded="name"/>
          </labels>
          <ports>
            <pointPort x="5" y="15" />
            <pointPort x="15" y="5" />
            <pointPort x="25" y="15" />
            <pointPort x="15" y="25" />
            <pointPort x="15" y="15" />
          </ports>

        </graphics>
        <logic>
          <generalizations>

            <parent parentName="reqnFeatured" />

          </generalizations>
        </logic>
      </node>
      <node name="reqnParent" displayedName="Parent Node" >
        <graphics>

          <!--<description>as leaf - for the possibility of future node extension</description>-->
          <picture sizex="30" sizey="30" >
            <external file="qml/reqnParentClass.qml"/>
	  </picture>
          <labels>
            <label x="5" y="10" textBinded="name"/>
          </labels>
          <ports>
            <pointPort x="5" y="15" />
            <pointPort x="15" y="5" />
            <pointPort x="25" y="15" />
            <pointPort x="15" y="25" />
            <pointPort x="15" y="15" />
          </ports>

        </graphics>
        <logic>
          <generalizations>
            
              <parent parentName="reqnFeatured" />
          
          </generalizations>
          <!--<associations>
            <assoc_ref idref="aP2N_Parent" />
          </associations>-->
        </logic>
      </node>
      <node name="reqnParentMandatory" displayedName="Parent of Mandatory Features" >
        <graphics>

          <!--<description>parent of mandatory features graphical representation</description>-->
          <picture sizex="30" sizey="30" >
            <external file="qml/reqnParentMandatoryClass.qml"/>
          </picture>
          <labels>
            <label x="5" y="10" textBinded="name"/>
          </labels>
          <ports>
            <pointPort x="5" y="15" />
            <pointPort x="15" y="5" />
            <pointPort x="25" y="15" />
            <pointPort x="15" y="25" />
            <pointPort x="15" y="15" />
          </ports>

        </graphics>
        <logic>
          <generalizations>

            <parent parentName="reqnParent" />

          </generalizations>
        </logic>
      </node>
      <node name="reqnParentOr" displayedName="Parent of Or-Features" >
        <graphics>

          <picture sizex="30" sizey="30" >
				<external file="qml/reqnParentOrClass.qml"/>
          </picture>
          <labels>
            <label x="5" y="10" textBinded="name"/>
          </labels>
          <ports>
            <pointPort x="5" y="15" />
            <pointPort x="15" y="5" />
            <pointPort x="25" y="15" />
            <pointPort x="15" y="25" />
            <pointPort x="15" y="15" />
          </ports>

        </graphics>
        <logic>
          <generalizations>

            <parent parentName="reqnParent" />

          </generalizations>
        </logic>
      </node>
      <node name="reqnParentOptional" displayedName="Parent of Optional Features" >
        <graphics>

          <picture sizex="30" sizey="30" >
            <external file="qml/reqnParentOptionalClass.qml"/>
          </picture>
          <labels>
            <label x="5" y="10" textBinded="name"/>
          </labels>
          <ports>
            <pointPort x="5" y="15" />
            <pointPort x="15" y="5" />
            <pointPort x="25" y="15" />
            <pointPort x="15" y="25" />
            <pointPort x="15" y="15" />
          </ports>

        </graphics>
        <logic>
          <generalizations>

            <parent parentName="reqnParent" />

          </generalizations>
        </logic>
      </node>
      <node name="reqnParentAlternative" displayedName="Parent of Alternative Features" >
        <graphics>

          <picture sizex="30" sizey="30" >
            <external file="qml/reqnParentAlternativeClass.qml"/>
          </picture>
          <labels>
            <label x="5" y="10" textBinded="name"/>
          </labels>
          <ports>
            <pointPort x="5" y="15" />
            <pointPort x="15" y="5" />
            <pointPort x="25" y="15" />
            <pointPort x="15" y="25" />
            <pointPort x="15" y="15" />
          </ports>

        </graphics>
        <logic>
          <generalizations>

            <parent parentName="reqnParent" />

          </generalizations>
        </logic>
      </node>
      <node name="reqnParentOpAlternative" displayedName="Parent of Optional-Alternative Features" >
        <graphics>

          <picture sizex="30" sizey="30" >
            <external file="qml/reqnParentOpAlternativeClass.qml"/>
          </picture>
          <labels>
            <label x="5" y="10" textBinded="name"/>
          </labels>
          <ports>
            <pointPort x="5" y="15" />
            <pointPort x="15" y="5" />
            <pointPort x="25" y="15" />
            <pointPort x="15" y="25" />
            <pointPort x="15" y="15" />
          </ports>

        </graphics>
        <logic>
          <generalizations>

            <parent parentName="reqnParent" />

          </generalizations>
        </logic>
      </node>
      <edge name="reqeP2N" displayedName="Feature P2N Relationship" >
        <graphics>
          <lineType type="solidLine"/>
        </graphics>
        <logic>

          <generalizations>
            <parent parentName="KernelDiagram::Relationship"/>
          </generalizations>
          <!--<possibleEdges>

            <possibleEdge beginName="reqnFeatured" endName="reqnParent" directed="true"/>
            

          </possibleEdges>-->
          <associations endType="open_arrow" beginType="no_arrow">
            <association beginName="reqnFeatured" endName="reqnFeatured"/>
          </associations>
        </logic>
      </edge>
    </graphicTypes>

  </diagram>
</metamodel>


